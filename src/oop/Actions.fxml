<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.Actions">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@ActionsBckg.jpg" />
         </image>
      </ImageView>
      <ProgressBar fx:id="progressBar" layoutY="395.0" opacity="0.95" prefHeight="10.0" prefWidth="600.0" progress="0.0" />
      <Label fx:id="labelChoose" layoutX="14.0" layoutY="14.0" prefHeight="22.0" prefWidth="109.0" text="Вибрано " textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="labelEntity" layoutX="112.0" layoutY="14.0" text="Нікого!" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="dig" layoutX="512.0" layoutY="361.0" mnemonicParsing="false" onAction="#dig" prefHeight="25.0" prefWidth="60.0" text="Dig!" />
      <Button fx:id="back" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="60.0" text="Back" />
      <Button fx:id="seekFood" layoutX="432.0" layoutY="361.0" mnemonicParsing="false" onAction="#seekFood" text="Get Food" />
      <Label fx:id="item" layoutX="15.0" layoutY="143.0" prefHeight="60.0" prefWidth="433.0" textFill="WHITE">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
